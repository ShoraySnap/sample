      "Code": "# Load the Python Standard and DesignScript Libraries\r\nimport sys\r\nsys.path.append(\"C:\\Program Files (x86)\\IronPython 2.7\\Lib\")\r\n# Load the Python Standard and DesignScript Libraries\r\nimport sys\r\nimport clr\r\nimport json\r\nclr.AddReference('ProtoGeometry')\r\nfrom Autodesk.DesignScript.Geometry import *\r\n\r\n\r\nclr.AddReference(\"RevitAPIIFC\")\r\nclr.AddReference(\"RevitAPI\")\r\nfrom Autodesk.Revit.DB import *\r\nfrom Autodesk.Revit.DB.Visual import *\r\nfrom Autodesk.Revit.DB.IFC import *\r\n\r\nclr.AddReference('RevitServices')\r\nimport RevitServices\r\nfrom RevitServices.Persistence import DocumentManager\r\nfrom RevitServices.Transactions import TransactionManager\r\nfrom System.Collections.Generic import Dictionary\r\n\r\ndoc = DocumentManager.Instance.CurrentDBDocument\r\nview = doc.ActiveView\r\n# The inputs to this node will be stored as a list in the IN variables.\r\n\r\nITEM_LIMIT = 150\r\n\r\nCATEGORY = [BuiltInCategory.OST_Levels, BuiltInCategory.OST_Walls, BuiltInCategory.OST_Floors, BuiltInCategory.OST_Columns,BuiltInCategory.OST_Railings]\r\n\r\ndef IsElementsVisibleInCurrentView(category):\r\n\tview = doc.ActiveView\r\n\tcategoryFilter = ElementCategoryFilter(category)\r\n\tcollector = FilteredElementCollector( doc, view.Id).WhereElementIsNotElementType().WherePasses(categoryFilter)\r\n\treturn collector.ToElements()\r\n\t\r\ndef getVisibleElements( category):\r\n\tobj = IsElementsVisibleInCurrentView(category)\r\n\treturn obj\r\n\t\r\ndef tolist(obj):\r\n\tif hasattr(obj,\"__iter__\"):return obj\r\n\telse:\r\n\t\treturn [obj]\r\n\t\r\n# get Elements present\r\nlevels = FilteredElementCollector(doc).OfCategory(CATEGORY[0]).WhereElementIsNotElementType().ToElements()\r\nwalls = getVisibleElements(CATEGORY[1] )\r\nfloors = getVisibleElements(CATEGORY[2] )\r\ncolumns = getVisibleElements(CATEGORY[3] )\r\n# temp fix for railing => not coming in filteredCollector\r\n# category != BuiltInCategory.OST_Railings \r\n# revit.elements.unknownelement\r\n\r\n\r\nelementsList = [levels, walls, floors,columns ] \r\n\r\nsingleFurnitureList = []\r\nfurnitureSetMap = {}\r\nfilteredFurnitures = []\r\ndirectFurnitureData = []\r\ndebug = []\r\n\r\n\r\n\r\n###### Inputs ######\r\n\r\nnames = {0:\"levels\", 1:\"walls\", 2:\"floors\",  3:\"columns\" }\r\n\r\n\r\ndata = {}\r\noutlist = []\r\n\r\nkeys = []\r\nopt = Options()\r\ndef getHeightFromBoundingBox(element):\r\n\tbbox = element.get_BoundingBox(view)\r\n\tif bbox :\r\n\t\trightUpperCorner = [bbox.Max[idx]*304.8 for idx in range(3)]\r\n\t\tleftBottomCorner = [bbox.Min[idx]*304.8 for idx in range(3)]\r\n\t\t#print \"MAX\", rightUpperCorner\r\n\t\t#print \"MIN\", leftBottomCorner\r\n\t\t\t\r\n\t\theight = rightUpperCorner[2] - leftBottomCorner[2]\r\n\t\t\t#print \"boundingBox:\", center\r\n\t\treturn height\r\n\telse:\r\n\t\t\t\r\n\t\treturn 0\r\ndef getColumnData(column):\r\n\tnoOfFaces = 0\r\n\tdef handleSolid(solid, noOfFaces):\r\n\t\t\tcolumnBottomFace = []\r\n\t\t\t#print solid.Faces.Size\r\n\t\t\tfor f in solid.Faces:\r\n\t\t\t\tif (f.ComputeNormal(UV(0.5,0.5)).Z < 0):\r\n\t\t\t\t\tnoOfFaces += 1\r\n\t\t\t\t\t#if True: to get bottom face\r\n\t\t\t\t\tedges = f.GetEdgesAsCurveLoops()\r\n\t\t\t\t\tsortedCurveLoops = ExporterIFCUtils.SortCurveLoops(edges);\r\n\t\t\t\t\tif len(sortedCurveLoops) > 1:\r\n\t\t\t\t\t\tnoOfFaces += 1\r\n\t\t\t\t\tfor e in edges:\r\n\t\t\t\t\t\tfor cl in e.GetCurveLoopIterator():\r\n\t\t\t\t\t\t\ttry:\r\n\t\t\t\t\t\t\t\tif isinstance(cl, Arc):\r\n\t\t\t\t\t\t\t\t\tpoints = cl.Tessellate()\r\n\t\t\t\t\t\t\t\t\tfor point in points:\r\n\t\t\t\t\t\t\t\t\t\tcolumnBottomVertices.append([point[idx]* 304.8 for idx in range(3)])\r\n\t\t\t\t\t\t\t\telse:\r\n\t\t\t\t\t\t\t\t\tcolumnBottomVertices.append([cl.GetEndPoint(0)[idx]* 304.8 for idx in range(3)])\r\n\t\t\t\t\t\t\texcept Exception as e:\r\n\t\t\t\t\t\t\t\tprint e\r\n\t\t\t\t\tbreak\r\n\t\t\treturn columnBottomFace, noOfFaces\r\n\t\t\t\t\r\n\tcolumnBottomVertices = []\r\n\tfor geo in column.get_Geometry(opt):\r\n\t\tif isinstance(geo, Solid):\r\n\t\t\t\tbottomFace, noOfFaces = handleSolid(geo, noOfFaces)\r\n\t\t\t\tcolumnBottomVertices += bottomFace\r\n\t\tif isinstance(geo, GeometryInstance):\r\n\t\t\ttransform = column.GetTransform()\r\n\t\t\tfor g in geo.GetInstanceGeometry():\r\n\t\t\t\tif isinstance(g, Solid):\r\n\t\t\t\t\tbottomFace, noOfFaces = handleSolid(g, noOfFaces)\r\n\t\t\t\t\t\r\n\t\t\t\t\tcolumnBottomVertices += bottomFace\t\r\n\t\t\t\t\r\n\t#print \"column\",noOfFaces, len(columnBottomVertices), columnBottomVertices, \r\n\theight = 0\r\n\tif column.get_Parameter(BuiltInParameter.INSTANCE_LENGTH_PARAM):\r\n\t\theight = column.get_Parameter(BuiltInParameter.INSTANCE_LENGTH_PARAM).AsDouble()*304.8\r\n\tif height == 0:\r\n\t\theight = getHeightFromBoundingBox(column)\r\n\t\t\r\n\treturn columnBottomVertices, height, noOfFaces\r\n\t\t\t\t\r\n\t\r\n\r\n# get map of names and element\r\nfor i in range(0,len(elementsList)):\r\n \tnum = 1\r\n \tkey = names[i] + \"-\" +str(num)\r\n\tif i == 3:\r\n\t\tfor element in range(0,len(elementsList[i]), ITEM_LIMIT):\r\n\t\t\tstart = element\r\n\t\t\tend = element + ITEM_LIMIT if element + ITEM_LIMIT < len(elementsList[i]) else len(elementsList[i])\r\n\t\t\tcolumns = []\r\n\t\t\tfor ele in range(start, end):\r\n\t\t\t\tcolumn = elementsList[i][ele]\r\n\t\t\t\tcolumnId = column.Id.ToString()\r\n\t\t\t\tcolumnBottomVertices, height, noOfFaces = getColumnData(column)\r\n\t\t\t\t#print noOfFaces\r\n\t\t\t\tcreateWithGeometry = True if noOfFaces > 1 else False\r\n\t\t\t\tc = json.dumps({\"type\": column.Name.ToString(),  \"height\": height,\"columnBottomFace\" : columnBottomVertices, \"elementId\": columnId, \"createWithGeometry\" : createWithGeometry})\r\n\t\t\t\tcolumns.append(c)\r\n\t\t\t\t#print \"--\"*10\r\n\t\t\t\t\r\n\t\t\tdata[key] = columns\r\n\t\t\tkeys.append(key)\r\n\t\t\tnum += 1\r\n\t\t\tkey =  names[i] + \"-\" +str(num)\r\n\telse:\r\n\t\tfor element in range(0,len(elementsList[i]), ITEM_LIMIT):\r\n\r\n\t\t\tdata[key] = []\r\n\t\t\tstart = element\r\n\t\t\tend = element + ITEM_LIMIT if element + ITEM_LIMIT < len(elementsList[i]) else len(elementsList[i])\t\r\n\t\t\tfor elem in range(start, end):\r\n\t\t\t\tdata[key].append(elementsList[i][elem])\r\n\t\t\tkeys.append(key)\r\n\t\t\tnum += 1\r\n\t\t\tkey =  names[i] + \"-\" +str(num)\r\n\r\n\t\t\t\r\n\r\n#outlist = [{\"metadata\" : json.dumps(keys)}]\r\n\r\nfor key, value in data.items():\r\n\tprint key, len(data[key])\r\n\td = Dictionary[str, object]({key:value})\r\n\toutlist.append(d)\r\n\r\n# Assign your output to the OUT variable.\r\nOUT =  outlist",
